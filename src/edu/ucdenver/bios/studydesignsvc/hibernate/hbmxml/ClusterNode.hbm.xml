<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>	
	<class name="edu.ucdenver.bios.webservice.common.domain.ClusterNode" table="CLUSTERING">		
		<id name="id" column="id"><generator class="native"/></id>				
		<property name="groupeName" column="group"/>		
		<property name="groupeSize" column="size"/>
		<property name="depth" column="depth"/>	
		<many-to-one name="studyDesign" column="uuid" not-null="true" unique="true"/>		 						
	</class>
</hibernate-mapping>