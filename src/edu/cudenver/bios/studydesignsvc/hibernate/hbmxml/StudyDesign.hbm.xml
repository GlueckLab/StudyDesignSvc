<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>	
	<class name="edu.cudenver.bios.studydesignsvc.domain.StudyDesign" table="tablestudydesign">
        <!--<id name="uuid" type="binary"/>-->
        <id name="uuid">
        	<generator class="assigned"/>
        </id>						
		<!-- <property name="flagSolveFor" column="flagSolveFor">			
		</property>-->
		<property name="hasGaussianCovariate" column="hasGaussianCovariate"/> 
		<property name="name" column="name" type="java.lang.String"/>	
		<!-- <one-to-one name="confidenceIntervalDescription" class="edu.cudenver.bios.studydesignsvc.domain.ConfidenceInterval" cascade="all"/>-->		
		<set name="confidenceIntervalDescriptions" table="tableConfidenceInterval" cascade="all" sort="unsorted" inverse="false" mutable="true" optimistic-lock="true" embed-xml="true">
		  <key column="uuid" on-delete="noaction" unique="true"/> 
		  <one-to-many class="edu.cudenver.bios.studydesignsvc.domain.ConfidenceInterval" not-found="exception" embed-xml="true" /> 
		</set>			
	</class>
</hibernate-mapping>